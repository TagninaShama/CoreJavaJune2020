//System.out.println("Students! ");
		int intVar1 = 10;
		int intVar2 = 5;
		byte byteVar1 = 5;
		float floatVar1 = 200.00F;
		double doubleVar1 = 0.1;
		
		System.out.println(byteVar1/intVar2-intVar1);
		System.out.println((byteVar1*byteVar1) +(intVar2 * intVar2));
		System.out.println((int) floatVar1 % byteVar1);
		System.out.println(1 / 2 * floatVar1);
		System.out.println((int)(intVar1/ floatVar1));
		
		
		String s1 = "65";
		char c1 = 54;
		char c2 = 33;
		char c3 = 'A';
		System.out.println((char)( c1 + c2)); // 1.
		System.out.println(c1); // 2.
		System.out.println((char)c1); // 3.
		System.out.println(c2); // 4.
		System.out.println(c2+1); // 5.
		System.out.println(c3+1); // 6.
		System.out.println((char)c3+1); // 7.
		System.out.println((char)(c3+1)); // 8.
		System.out.println((char)(c2+1)); // 9.
		System.out.println(s1 + 4 * 2.5); // 10.
		System.out.println(s1 + 4 + 2.5); // 11.
		System.out.println(s1 + ( 4 + 2.5));


	}

}
